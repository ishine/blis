" lsl             %[cs_a], %[cs_a], #3            \n\t" \
" lsl             %[rs_b], %[rs_b], #3            \n\t" \
" lsl             %[rs_c], %[rs_c], #3            \n\t" \
" lsl             %[cs_c], %[cs_c], #3            \n\t" \
"                                                 \n\t" \
" mov             %[c_ld], %[c]                   \n\t" \
